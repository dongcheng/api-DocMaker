<%@page import="com.melonlee.api.bean.FunctionBean"%><%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><%@include file="common.jsp"%></head><%	ArrayList<FunctionBean> func = (ArrayList<FunctionBean>)request.getAttribute("funs"); %><body>	<div class="container">		<div class="header">			<ul class="nav nav-pills pull-right">				<li><a href="index.jsp">首页</a></li>				<li class="active"><a href="pros.do">项目</a></li>			</ul>			<h3 class="text-muted">API Doc Maker</h3>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<ul class="breadcrumb">						<li><a href="#">主页</a> <span class="divider"></span></li>						<li><a href="modules.do?pID=<%=request.getParameter("pID")%>"><%=func.size()>0 ? func.get(0).getpName() : "未获取到"%></a> <span class="divider"></span></li>						<li class="active"><%=func.size()>0 ? func.get(0).getmName()+"模块" : "未获取到"%>						</li>					</ul>				</div>			</div>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<table class="table table-hover table-bordered">						<thead>							<tr>								<th>编号</th>								<th>接口</th>								<th>创建时间</th>								<th>状态</th>							</tr>						</thead>						<tbody>						<%								int i = 0;			for (FunctionBean fun : func) {				i++;							%>							<tr>								<td><%=i%></td>								<td><a href="#"><%=fun.getTitle()%></a></td>								<td><%=fun.getCreateDate()%></td>								<td>									<a class="btn  btn-danger " href="delFun.do?id=<%=fun.getId()%>&mID=<%=fun.getmID() %>"									role="button">删除接口</a>								</td>							</tr>							<%								}							%>						</tbody>					</table>				</div>			</div>		</div>		<p>			<a class="btn  btn-success" href="modifyFun.jsp" role="button">添加接口</a>			<a class="btn  btn-info" href="createPro.jsp" role="button">预览文档</a>			<a class="btn  btn-warning" href="createPro.jsp" role="button">下载文档</a>		</p>		<div class="footer">			<p>&copy; melonlee.com 2014</p>		</div>	</div>	<!-- /container --></body></html>