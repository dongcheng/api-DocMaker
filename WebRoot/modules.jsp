<%@page import="com.melonlee.api.bean.ModuleBean"%><%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><%@include file="common.jsp"%></head><%	ArrayList<ModuleBean> modules = new ArrayList();if(null!=request.getAttribute("modules")){ 	modules = (ArrayList<ModuleBean>) request.getAttribute("modules");}%><body>	<div class="container">		<div class="header">			<ul class="nav nav-pills pull-right">				<li><a href="index.jsp">首页</a>				</li>				<li class="active"><a href="pros.do">项目</a>				</li>			</ul>			<h3 class="text-muted">API Doc Maker</h3>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<ul class="breadcrumb">						<li><a href="pros.do">主页</a> <span class="divider"></span></li>						<li class="active"><%=   modules.size()>0 ? modules.get(0).getpName() : null!=request.getAttribute("proName") ? request.getAttribute("proName") : "" %></li>					</ul>				</div>			</div>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<table class="table table-hover table-bordered">						<thead>							<tr>								<th>编号</th>								<th>模块</th>								<th>创建时间</th>								<th>状态</th>							</tr>						</thead>						<tbody>							<%								int i = 0;			for (ModuleBean pro : modules) {				i++;							%>							<tr>								<td><%=i%></td>								<td><a href="funs.do?mID=<%=pro.getId()%>&pID=<%=request.getParameter("pID") %>"><%=pro.getTitle()%></a></td>								<td><%=pro.getCreateDate()%></td>								<td>									<a class="btn  btn-warning" href="getModule.do?id=<%=pro.getId()%>&pID=<%=request.getParameter("pID") %>"									role="button">编辑</a> <a class="btn  btn-info" href="createFun.jsp?mID=<%=pro.getId()%>&pID=<%=request.getParameter("pID") %>"									role="button">添加接口</a>																	</td>							</tr>							<%								}							%>						</tbody>					</table>				</div>			</div>		</div>		<p>			<a class="btn  btn-success" href="createModule.jsp?pID=<%=request.getAttribute("proID") %>" role="button">添加模块</a>		</p>		<div class="footer">			<p>&copy; melonlee.com 2014</p>		</div>	</div>	<!-- /container --></body></html>