<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><%@include file="common.jsp"%></head><body>	<div class="container">		<div class="header">			<ul class="nav nav-pills pull-right">				<li><a href="index.jsp">首页</a></li>				<li class="active"><a href="pros.do">项目</a></li>			</ul>			<h3 class="text-muted">API Doc Maker</h3>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<ul class="breadcrumb">						<li class="active">编辑接口信息</li>					</ul>				</div>			</div>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<form class="form-horizontal" action="saveFun.do" method="post">						<input id="mID" type="hidden" name="mID"							value="<%=request.getParameter("mID")%>" />						<div class="control-group">							<p>1.标题</p>							<div class="controls">								<input id="title" type="text" name="title" size="45" />							</div>						</div>						<br />						<div class="control-group">							<p>2.Http请求方式</p>							<div class="controls">								<select name="httpMethod">									<option value="GET">GET</option>									<option value="POST">POST</option>									<option value="DELETE">DELETE</option>									<option value="UPDATE">UPDATE</option>								</select>							</div>						</div>						<br />						<div class="control-group">							<p>3.是否需要登录验证</p>							<div class="controls">								<select name="isLogin">									<option value="0">否</option>									<option value="1">是</option>								</select>							</div>						</div>						<br />						<div class="control-group">							<p>4.Url调用事例</p>							<div class="controls">								<input id="url" type="text" name="url" size="45" />							</div>						</div>						<br />						<div class="control-group">							<p>5.注意事项</p>							<div class="controls">								<textarea rows="5" cols="80" name="params"></textarea>							</div>						</div>						<br />						<div class="control-group">							<div class="controls">								<button type="submit" class="btn btn-success">提交</button>								 <button type="button" class="btn btn-info" onClick="javascript:window.location.href='modules.do?pID=<%=request.getParameter("pID")%>'">取消</button>															</div>						</div>					</form>				</div>			</div>		</div><br/>		<div class="footer">			<p>&copy; melonlee.com 2014</p>		</div>	</div>	<!-- /container --></body></html>