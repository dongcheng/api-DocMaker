<%@page import="com.melonlee.api.bean.ModuleBean"%><%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><%@include file="common.jsp"%></head><%	ModuleBean module =  null;		if(null!=request.getAttribute("module")){		module = (ModuleBean)request.getAttribute("module");	} %><body>	<div class="container">		<div class="header">			<ul class="nav nav-pills pull-right">				<li class="active"><a href="index.jsp">首页</a></li>				<li><a href="pros.do">项目</a></li>			</ul>			<h3 class="text-muted">API Doc Maker</h3>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<ul class="breadcrumb">						<li class="active">编辑模块信息</li>					</ul>				</div>			</div>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<form class="form-horizontal" action="saveModule.do" method="post">						<input type="hidden" name="pID"							value="<%=null != request.getAttribute("proID") ? request					.getAttribute("proID") : request.getParameter("pID")%>">					<input type="hidden" name= "id" value="<%=null!=module ? module.getId() : "0"%>">						<div class="control-group">							<p>1.请填写模块名称</p>							<div class="controls">								<input id="title" type="text" name="title" size="45"  value="<%=null!=module ? module.getTitle() : "" %>" />							</div>						</div>						<br />						<div class="control-group">							<p>2.模块Url路径</p>							<div class="controls">								<input id="baseUrl" type="text" name="baseUrl" size="45"  value="<%=null!=module ?module.getBaseUrl():"/" %>"/>							</div>						</div>						<br />						<div class="control-group">							<p>3.模块说明</p>							<div class="controls">								<textarea rows="5" cols="80" name="descStr"><%=null!=module ? module.getDescStr() : "" %></textarea>							</div>						</div>						<br />						<div class="control-group">							<div class="controls">								<button type="submit" class="btn btn-success">提交</button>								<button type="button" class="btn btn-info" onClick="javascript:window.location.href='modules.do?pID=<%=request.getParameter("pID")%>'">取消</button>							</div>						</div>					</form>				</div>			</div>		</div>		<br />		<div class="footer">			<p>&copy; melonlee.com 2014</p>		</div>	</div>	<!-- /container --></body></html>