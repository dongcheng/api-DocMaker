<%@page import="com.melonlee.api.bean.ProjectBean"%><%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><%@include file="common.jsp"%></head><%	ProjectBean pro =  null;		if(null!=request.getAttribute("pro")){		pro = (ProjectBean)request.getAttribute("pro");	} %><body>	<div class="container">		<div class="header">			<ul class="nav nav-pills pull-right">				<li class="active"><a href="index.jsp">首页</a>				</li>				<li><a href="pros.do">项目</a>				</li>			</ul>			<h3 class="text-muted">API Doc Maker</h3>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<ul class="breadcrumb">						<li class="active">编辑项目信息</li>					</ul>				</div>			</div>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<form class="form-horizontal" action="savePro.do" method="post">										<input type="hidden" name= "id" value="<%=null!=pro ? pro.getId() : "0"%>">						<div class="control-group">							<p>1.请填写项目名称</p>							<div class="controls">								<input id="title" type="text" name="title" size="45" value="<%=null!=pro ? pro.getTitle() : "" %>" />							</div>						</div>						<br />						<div class="control-group">							<p>2.项目BaseUrl</p>							<div class="controls">								<input id="baseUrl" type="text" name="baseUrl" size="45" value="<%=null!=pro ?pro.getBaseUrl():"http://" %>" />							</div>						</div>						<br />						<div class="control-group">							<p>3.项目版本号</p>							<div class="controls">								<input id="version" type="text" name="version" size="45"  value="<%=null!=pro ? pro.getVersion() : "1.0"%>"/>							</div>						</div>						<br />						<div class="control-group">							<p>4.项目说明</p>							<div class="controls">								<textarea rows="5" cols="80" name="descStr"><%=null!=pro ? pro.getDescStr() : "" %></textarea>							</div>						</div>						<br />						<div class="control-group">							<div class="controls">								<button type="submit" class="btn btn-success">提交</button>								<button type="button" class="btn btn-info" onClick="javascript:window.location.href='pros.do'">取消</button>							</div>						</div>					</form>				</div>			</div>		</div>		<br />		<div class="footer">			<p>&copy; melonlee.com 2014</p>		</div>	</div>	<!-- /container --></body></html>