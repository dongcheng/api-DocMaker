<%@page import="com.melonlee.api.bean.ProjectBean"%><%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><%@include file="common.jsp"%></head><%	ArrayList<ProjectBean> pros = (ArrayList<ProjectBean>) request.getAttribute("pros");%><body>	<div class="container">		<div class="header">			<ul class="nav nav-pills pull-right">				<li><a href="index.jsp">首页</a></li>				<li class="active"><a href="pros.do">项目</a></li>			</ul>			<h3 class="text-muted">API Doc Maker</h3>		</div>		<div class="container-fluid">			<div class="row-fluid">				<div class="span12">					<table class="table table-hover table-bordered">						<thead>							<tr>								<th>编号</th>								<th>项目</th>								<th>创建时间</th>								<th></th>							</tr>						</thead>						<tbody>							<%								int i = 0;			for (ProjectBean pro : pros) {				i++;							%>							<tr>								<td><%=i%></td>								<td><a href="modules.do?pID=<%=pro.getId()%>"><%=pro.getTitle()%></a>								</td>								<td><%=pro.getCreateDate()%></td>								<td><a class="btn  btn-info"  target="_blank" href="createDoc.do?pID=<%=pro.getId()%>"									role="button">预览文档</a> <a class="btn  btn-danger" href="createDoc.do?pID=<%=pro.getId()%>"									role="button">下载文档</a>								</td>							</tr>							<%								}							%>						</tbody>					</table>				</div>			</div>		</div>		<p>			<a class="btn  btn-success" href="createPro.jsp" role="button">添加项目</a>		</p>		<div class="footer">			<p>&copy; melonlee.com 2014</p>		</div>	</div>	<!-- /container --></body></html>